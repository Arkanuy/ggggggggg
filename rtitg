World = {"QXGVB","BRYNP","QPOVM","OBLUZ","RBHWC","TAZZQ","SZFHN","QZPNU","KCLEG","YUPFI","RUOWL","PQQVL","IPDSH","QJWBI","IPDSS","CBDCF","IFCSG","ZCRUI","NFJXE","EMOQA","JSXKB","QHXCG","ILMBG","KAOBH","FWNVI"}
id_door = "Arkan55"
Delay_Join_World = 8000
World_Execute = "QXGVB"
take_float = false 

looping = true


max_player_in_world = 10
Delay_Escape = 60 --detik bukan milidetik
--==========PNB=========--
BlockID = 5666
coordinate_x = 2
coordinate_y = 2
mode_pnb = 2
Hit = 4
--=========DELAY========--
Delay_Punch = 200
Delay_Place = 130
Delay_Harvest = 130
Delay_Plant = 130
--=========SEED=========--
World_Seed = "KASEEDS100"
id_door_seed = "arkan55"
patokan_drop_seed = 1158 -- ID PATOKAN DROP
total_drop_seed = 200 -- JUMLAH YG MAU DI DROP
--=========PACK=========--
World_Pack = "KAPACKS100"
id_door_pack = "arkan55"
patokan_drop_pack = 1158 -- ID PATOKAN DROP

mode_buy_pack = 1
pack = "world_lock_10_pack"
price = 20000
item_pack = {242}
total_buy = 1
--========WEBHOOK========--
webhook = true
DiscordID = 829262268881567796 -- ganti jdi id discord kau
profile = "https://c.tenor.com/RJaiYJJV8xsAAAAC/saitama-pfp.gifl" -- klo mau ganti tinggal ganti

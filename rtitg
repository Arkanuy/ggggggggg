trash_list = {5036,7164,5044,7162,5028,5024,5026,5042,5038,5032,5034,5040,2,3,8846}

function wp(world,id)
    while getWorld().name ~= world:upper() do
        sendPacket(3,"action|join_request\nname|"..world:upper())
        sleep(Delay_Join_World)
    end
    move(3,0)
    sleep(400)
    while getTile(math.floor(getBot().x / 32),math.floor(getBot().y / 32)).fg == 6 do 
        sendPacket(3,"action|join_request\nname|"..world:upper().."|"..id:upper())
        sleep(1000)
    end
end

function fp(x,y)
    findPath(x,y)
    sleep(100)
end

function harvest()
    collectSet(true,3)
    for _, tile in pairs(getTiles()) do
        if tile.fg == BlockID+1 and tile.ready and findItem(BlockID) < 185 then
            findPath(tile.x,tile.y)
            sleep(Delay_Harvest)
            punch(0,0)
            kabur()
            sleep(100)
            automove()
        end
    end
    if findItem(BlockID) >= 185 then
        gopos()
        if mode_pnb == 1 then
            pnb1()
        elseif mode_pnb == 2 then
            pnb2()
        elseif mode_pnb == 3 then
            pnb3()
        end
    end
end

function trash(id)
    sendPacket(2,"action|trash\nitemID|"..id)
    sleep(1000)
    sendPacket(2,"action|dialog_return\ndialog_name|trash_item\nitemID|"..id.."|\ncount|"..findItem(id))
end

function ayotrash()
    for i, v in ipairs(trash_list) do
        if findItem(v) > 0 then
            trash(v)
            sleep(500)
        end
    end
end

function getcountp()
    count = 0
    for _, player in pairs(getPlayers()) do
        count = count + 1
    end
    return count
end

function kabur()
    if getcountp() >= max_player_in_world then
        local bb = getworld() 
        awl(":eyes: : Total Player in World **["..getWorld().name.."]** = "..getcountp().." People\n:loudspeaker: : Too Many Player in World, Go to **[EXIT]** for "..Delay_Escape.." Seconds","Auto Rotation Farm",15105570)
        sendPacket(3,"action|join_request\nname|EXIT")
        sleep(Delay_Escape * 1000)
        wp(bb,id_door)
        sleep(1000)
        harvest()
    end
end

function pnb3()
    collectSet(true,4)
    gopos()
    say("Gems : `2"..findItem(112))
    sleep(200)
    if findItem(112) >= price * total_buy + 700 and mode_buy_pack == 1 then
        godrop()
    end
    if findItem(BlockID) > 0 then
        place(BlockID,-1,-1)
        sleep(100)
        place(BlockID,0,-1)
        sleep(100)
        place(BlockID,1,-1)
        while findItem(BlockID) > 0 do
            if findItem(BlockID+1) >= total_drop_seed then
                dropseed()
                automove()
            end
            automove()
            kabur()
            for i=1,Hit do
                punch(-1,-1)
                gopos()
                sleep(Delay_Punch)
                punch(0,-1)
                sleep(Delay_Punch)
                punch(1,-1)
                sleep(Delay_Punch)
                automove()
            end  
            place(BlockID,-1,-1)
            sleep(Delay_Place)
            place(BlockID,0,-1)
            sleep(Delay_Place)
            place(BlockID,1,-1)
            sleep(Delay_Place)
        end
    end
    collectSet(false)
    cekblock()
    plant()
end

function pnb2()
    collectSet(true,4)
    gopos()
    say("Gems : `2"..findItem(112))
    sleep(200)
    if findItem(112) >= price * total_buy + 700 and mode_buy_pack == 1 then
        godrop()
    end
    if findItem(BlockID) > 0 then
        place(BlockID,-1,-1)
        sleep(100)
        place(BlockID,0,-1)
        sleep(120)
        place(BlockID,1,-1)
        sleep(100)
        while findItem(BlockID) > 0 do 
            gopos()
            if findItem(BlockID+1) == total_drop_seed then
                dropseed()
            end
            automove()
            kabur()
            local posx = math.floor(getBot().x/32)
            local posy = math.floor(getBot().y/32)
            while getTile(posx-1,posy-1).fg > 0 or getTile(posx-1,posy-1).bg > 0 do
                punch(-1,-1)
                sleep(Delay_Punch)
                automove()
                gopos()
            end
            place(BlockID,-1,-1)
            sleep(Delay_Place)
            while getTile(posx,posy-1).fg > 0 or getTile(posx,posy-1).bg > 0 do
                punch(0,-1)
                sleep(Delay_Punch)
                automove()
                gopos()
            end
            place(BlockID,0,-1)
            sleep(Delay_Place)
            while getTile(posx+1,posy-1).fg > 0 or getTile(posx+1,posy-1).bg > 0 do
                punch(1,-1)
                sleep(Delay_Punch)
                automove()
            gopos()
            end
            place(BlockID,1,-1)
            sleep(Delay_Place)
        end
    end
    collectSet(false)
    cekblock()
    plant()
end 

function pnb1()
    collectSet(true,2)
    gopos()
    say("Gems : `2"..findItem(112))
    sleep(200)
    if findItem(112) >= price * total_buy + 700 and mode_buy_pack == 1 then
        godrop()
    end 
    while findItem(BlockID) > 0 do
        gopos()
        if findItem(BlockID+1) >= total_drop_seed then
            dropseed()
        end 
        local posx = math.floor(getBot().x/32)
        local posy = math.floor(getBot().y/32)
        place(BlockID,0,-1)
        sleep(Delay_Place)
        while getTile(posx,posy-1).fg > 0 or getTile(posx,posy-1).bg > 0 do
            punch(0,-1)
            kabur()
            sleep(Delay_Punch)
            automove()
            gopos()
        end
    end
    collectSet(false)
    cekblock()
end

function cekblock()
    for _, tile in ipairs(getTiles()) do
        if tile.fg == BlockID or tile.bg == BlockID then
            findPath(tile.x,tile.y)
            sleep(1000)
            collect(3)
            if getTile(math.floor(getBot().x/32),math.floor(getBot().y/32)).fg > 0 or getTile(math.floor(getBot().x/32),math.floor(getBot().y/32)).bg > 0 then
                while getTile(math.floor(getBot().x/32),math.floor(getBot().y/32)).fg > 0 or getTile(math.floor(getBot().x/32),math.floor(getBot().y/32)).bg > 0  do
                    punch(0,0)
                    sleep(200)
                end
            end
            collect(2)
        end
    end
    plant()
end

function gopos()
    while math.floor(getBot().x/32) ~= coordinate_x-1 or math.floor(getBot().y/32)  ~= coordinate_y-1 do
        fp(coordinate_x-1,coordinate_y-1)
        sleep(1300)
        automove()
    end
end 

function plant()
    collectSet(true, 2)
    for _, tile in pairs(getTiles()) do
        if tile.fg == 0 and getTile(tile.x,tile.y+1).fg ~= 0 and getTile(tile.x,tile.y+1).fg ~= BlockID+1 and findItem(BlockID+1) > 0 then
            findPath(tile.x,tile.y)
            sleep(Delay_Plant)
            place(BlockID+1,0,0)
            sleep(80)
            automove()
            kabur()
        end
    end
    if take_float then
    	while scanfloat(BlockID) > 0 do
        	takefloat(BlockID)
    	end
    end
    harvest()
end



function dropseed()
    collectSet(false)
    wseed = getBot().world
    wp(World_Seed,id_door_seed)
    for _, tile in pairs(getTiles()) do 
        if tile.fg == patokan_drop_seed or tile.bg == patokan_drop_seed then
            findPath(tile.x-1,tile.y)
            sleep(500)
        end
    end
    drop(BlockID+1,findItem(BlockID+1))
    sleep(1000)
    while findItem(BlockID+1) >= total_drop_seed do
        move(-1,0)
        sleep(800)
        drop(BlockID+1,findItem(BlockID+1))
        sleep(800)
    end
    awl(":farmer: :"..getBot().name..", (level "..getBot().level..")\n:satellite: :Online, ("..getBot().ping.." ms)\n:gem: :"..findItem(112).."\n:loudspeaker: : Dropping Seed to **["..World_Seed.."]**\n:mega: : Successfully Dropping Seed\n:mag: : Total Dropped Seed in **["..World_Seed.."]** = "..scan(BlockID + 1),"Auto Rotation Farm\n:earth_asia: Current World ["..wseed.."]","https://www.digitrac.in/pub/media/magefan_blog/Wheat_image_1.jpg","Arkanuy#8243 | Auto Rotation",9807270)
    sleep(2000)
    wp(wseed,id_door)
    sleep(1000)
    collectSet(true,4)
    gopos()
end

function scan(id)
    count = 0
    for _, object in pairs(getObjects()) do
        if object.id == id then
        count = count + object.count
        end
    end
    return count
end

function automove()
    if getBot().state ~= 5 then
        awl2(":farmer: :"..getBot().name.."\n:satellite: :Disconnected\n:loudspeaker: :Trying to reconnected again","Auto Rotation Farm\n:satellite: : DISCONNECTED !!!!!!!!!!!!!!","https://c.tenor.com/HWdSvD9Wg20AAAAM/one-punch-man-ok.gif","Arkanuy#8243 | Auto Rotation",15158332)
        while getBot().state ~= 5 do
            connect()
            sleep(50000)
            if getBot().state == 5 then
                awl2(":farmer: :"..getBot().name.."\n:satellite: :Online\n:loudspeaker: :Successfully Recconected Again","Auto Rotation Farm\n:satellite: : CONNECTED AGAIN !!!!!","https://c.tenor.com/IqiptPCV4nsAAAAM/saitama-yeah.gif","Arkanuy#8243 | Auto Rotation",3066993)
            end
        end
    end
    while getTile(math.floor(getBot().x/32),math.floor(getBot().y/32)).fg == 6 do
        warp(getWorld().name.."|"..id_door)
        sleep(1000)
        move(4,0)
        sleep(500)
    end
end

function godrop()
    wpack = getBot().world
    collectSet(false)
    wp(World_Pack,id_door_pack)
    for _, tile in pairs(getTiles()) do
        if tile.fg == patokan_drop_pack or tile.bg == patokan_drop_pack then
            findPath(tile.x,tile.y)
            sleep(400)
        end
    end
    buypack()
    droppack()
    awl(":farmer: :"..getBot().name..", (level "..getBot().level..")\n:satellite: :Online, ("..getBot().ping.." ms)\n:gem: :"..findItem(112).."\n:loudspeaker: :Buying Pack\n:package: :"..pack.."\n:toolbox: :Successfully Buying Pack","Auto Rotation Farm\n:earth_asia: Current World : **["..wpack.."]**","https://c.tenor.com/lD3Q3jF-MWoAAAAC/saitama-one-punch-man.gif","Arkanuy#8243 | Auto Rotation",16705372)
    wp(wpack,id_door)
    sleep(1000)
    gopos()
    collectSet(true,4)
end

function buypack()
    while findItem(112) > price do
        sendPacket(2,"action|buy\nitem|"..pack)
        sleep(1800)
        if findItem(item_pack[1]) < 1 then
            sendPacket(2,"action|buy\nitem|upgrade_backpack")
            sleep(1500)
        end
    end
end


function droppack()
    for i = 1,2 do
        for z, z in ipairs(item_pack) do
            if findItem(z) >= 1 then
                drop(z,findItem(z))
                sleep(1500)
                drop(z,findItem(z))
                sleep(1000)
                while findItem(z) >= 1 do
                    move(-1,0)
                    sleep(500)
                    drop(z,findItem(z))
                    sleep(1000)
                    if findItem(5030) >= 1 then
                        drop(5030)
                        sleep(500)
                    end
                end
            end
        end
    end
end

function takefloat(id)
    collectSet(true,4)
    for _, obj in pairs(getObjects()) do
        if obj.id == id then
            findPath(math.floor(obj.x/32),math.floor(obj.y/32))
            sleep(200)
        end
        if findItem(BlockID) >= 185 then
            if mode_pnb == 1 then
                pnb1()
            elseif mode_pnb == 2 then
                pnb2()
            elseif mode_pnb == 3 then
                pnb3()
            end
        end
    end
end

function scanfloat(ID) 
    count = 0
    for _, obj in pairs(getObjects()) do
        if obj.id == ID then
            count = count + obj.count
        end
    end
    return count
end


